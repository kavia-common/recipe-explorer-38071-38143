# PUBLIC_INTERFACE
# Example environment variables for frontend_app.
# Copy this file to .env and adjust values for your environment.

# Port the dev server binds to
REACT_APP_PORT=3000

# Toggle source maps (keep false for CI/low memory)
REACT_APP_ENABLE_SOURCE_MAPS=false

# Node environment (development/production)
REACT_APP_NODE_ENV=development

# Base URLs for API and app (if used by future features)
REACT_APP_API_BASE=https://api.example.com
REACT_APP_BACKEND_URL=https://api.example.com
REACT_APP_FRONTEND_URL=https://frontend.example.com
REACT_APP_WS_URL=wss://ws.example.com

# Misc flags
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz
REACT_APP_FEATURE_FLAGS={}
REACT_APP_EXPERIMENTS_ENABLED=false
