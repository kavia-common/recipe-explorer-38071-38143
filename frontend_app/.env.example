# Example environment configuration for frontend_app
# Copy to .env and adjust as needed. Do NOT commit actual secrets.

# Networking
REACT_APP_PORT=3000
REACT_APP_NODE_ENV=development
REACT_APP_TRUST_PROXY=false

# Source maps (disable in CI to save memory)
REACT_APP_ENABLE_SOURCE_MAPS=false

# URLs
REACT_APP_API_BASE=https://api.example.com
REACT_APP_BACKEND_URL=https://backend.example.com
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:3000

# Misc
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS={}
REACT_APP_EXPERIMENTS_ENABLED=false
