# PUBLIC_INTERFACE
# This example lists environment variables consumed by the frontend App.
# Copy to ".env" and adjust values for your environment. Do not commit real secrets.

# Server binding and port
REACT_APP_PORT=3000
HOST=0.0.0.0
BROWSER=none

# Environment mode flags
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1

# Source maps (disable in CI/low memory to save RAM)
REACT_APP_ENABLE_SOURCE_MAPS=false

# Logging and healthcheck
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health

# Feature toggles
REACT_APP_FEATURE_FLAGS={}
REACT_APP_EXPERIMENTS_ENABLED=false

# Public URLs
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=
REACT_APP_WS_URL=
